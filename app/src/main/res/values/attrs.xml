<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomToolbar">
        <attr name="leftText" format="string" />
        <attr name="centerText" format="string" />
        <attr name="rightText" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomExcel">
        <attr name="show_search" format="boolean" />
        <attr name="header_background" format="color" />
        <attr name="header_min_height" format="dimension" />
        <attr name="list_item_min_height" format="dimension" />
        <attr name="header_max_lines" format="integer" />
        <attr name="stripe_list" format="boolean" />
        <attr name="first_column_index" format="boolean" />
        <attr name="tab_items" format="reference" />
        <attr name="last_column_type">
            <enum name="skip" value="0" />
            <enum name="del" value="1" />
            <enum name="edit" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="Preference">
        <attr name="necessary" format="boolean" />
        <attr name="title" format="string" />
        <attr name="content_value" format="string" />
        <attr name="content_type">
            <enum name="text" value="0" />
            <enum name="edit" value="1" />
            <enum name="choice" value="2" />
            <enum name="drop" value="3" />
        </attr>
        <attr name="android:inputType" />
        <attr name="choices" format="reference" />
        <attr name="spinner_items" format="reference" />
    </declare-styleable>
</resources>